
# Notifyr environment example
# Copy this file to `.env` and adjust values for your environment.

NOTIFYR_IP=127.0.0.54

# Application mode: one of 'dev', 'prod', 'test'
MODE=dev

# URLs
PROD_URL=
DEV_URL=

# Naming
DOMAIN_NAME=notifyr
USERNAME=notifyr

# Directories
BASE_DIR=./
ASSETS_DIR=assets/
OBJECTS_DIR=objects/

# Security
# Set to 'true' or 'false'. Recommended 'true' in production.
SECURITY_FLAG=false

# Server
HTTP_MODE=HTTP
HTTPS_CERTIFICATE=
HTTPS_KEY=

# Asset storage: 's3', 'github', 'local', or 'ftp'
ASSET_MODE=s3

# S3 / Minio
S3_CRED_TYPE=MINIO
S3_ENDPOINT=127.0.0.1:9000
S3_REGION=
S3_TO_DISK=false

# Minio specific
MINIO_STS_ENABLE=false
MINIO_SSL=false

# Graphiti
GRAPHITI_MAX_COROUTINES=10
GRAPHITI_SERVICE=neo4j

# Databases & services
MONGO_HOST=mongodb
REDIS_HOST=redis
RABBITMQ_HOST=rabbitmq
MEMCACHED_HOST=memcached
POSTGRES_HOST=postgres
GRAPHITI_HOST=neo4j

# Protocol: 'bolt' or 'neo4j+s'
GRAPHITI_PROTOCOL=bolt
# Service: 'neo4j' or 'memgraph'
GRAPHITI_SERVICE=neo4j

# Minio storage limit default 500MB
ASSETS_STORAGE_LIMIT=
STATIC_STORAGE_LIMIT=

#Data processing
INSTALL_DOCLING=false

# Celery configuration
# Broker: 'redis' or 'rabbitmq'
BROKER_PROVIDER=rabbitmq

# Optional full URLs. If empty, the app will construct defaults from other vars.
CELERY_BACKEND_URL=

CELERY_RESULT_EXPIRES=86400
CELERY_VISIBILITY_TIMEOUT=7200

# APS scheduler
APS_ACTIVATED=true
APS_JOBSTORE=redis

# Database credentials
POSTGRES_USER=
POSTGRES_PASSWORD=

MONGO_INITDB_ROOT_USERNAME=
MONGO_INITDB_ROOT_PASSWORD=

RABBITMQ_DEFAULT_USER=
RABBITMQ_DEFAULT_PASS=

NEO4J_USER=
NEO4J_PASSWORD=

REDIS_USER=
REDIS_PASSWORD=

REDIS_NOTIFYR_PASSWORD=

MINIO_VAULT_PASSWORD=

# API Keys.
GOOGLE_SAFE_SEARCH_API_KEY=
IPINFO_API_KEY=


# --- End of example ---
