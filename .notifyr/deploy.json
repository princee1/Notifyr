{
    "deployment_name": "Notifyr - Community Edition",
    "tier": "Base",
    "environment": "production",
    "deployment_type": "docker-compose",
    "version": "1.2.0",
    "description": "Configuration for the high-availability, microservice architecture.",
    "scaling": {
        "app": 1,
        "worker": 0,
        "balancer": 1
    },
    "agentic": {
        "knowledge_graph": false,
        "vector": true
    },
    "capabilities": {
        "object": false,
        "email": false,
        "twilio": false,
        "notification": false,
        "message": false,
        "agentic": true,
        "webhook": false,
        "workflow": false,
        "chat":false
    },
    "resource_limits": {
        "cpu_limit": "500m",
        "memory_limit": "512Mi"
    },
    "database":{
        "postgres": true,
        "redis": true,
        "rabbitmq": true,
        "qdrant": true,
        "bolt": true,
        "minio": true,
        "mongodb": true
    }
}